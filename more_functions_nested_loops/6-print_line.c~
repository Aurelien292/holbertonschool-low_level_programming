#include "main.h"

/**
 * print_most_numbers - Prints the numbers from 0 to 9
 * followed by a new line
 *
 * Description: Uses _putchar to print each number
 */

void print_line(int n)

{
int Nb = 0;
if (Nb <= 0)
{
putchar('\n');
}
else
{
for (int i = 0; i < Nb ; i++)
{
putchar('_');
}
putchar('\n');
}
return (0);
}
